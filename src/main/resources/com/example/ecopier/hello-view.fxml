<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" fillWidth="false" prefHeight="692.0" prefWidth="395.0" spacing="30.0" style="-fx-background-color: beige;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.ecopier.CopyController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Browse copy list file" />
    <Button mnemonicParsing="false" onAction="#browseCopyList" style="-fx-background-color: DARKSEAGREEN;" text="Browse" />
    <Label alignment="TOP_LEFT" prefHeight="14.0" prefWidth="104.0" text="Browse ethalon file" />
    <Button accessibleRole="MENU" mnemonicParsing="false" onAction="#browseEthalon" rotate="-0.6" style="-fx-background-color: DARKSEAGREEN;" text="Browse" />
    <Label text="Type month and year" />
    <TextField fx:id="textField" prefHeight="26.0" prefWidth="116.0" />
    <Button mnemonicParsing="false" onAction="#copy" style="-fx-background-color: LIGHTGREEN;" text="Copy" />
    <Label fx:id="filesQuan" textAlignment="RIGHT" />
    <Label fx:id="firstNum" />
    <Label fx:id="lastNum" />
   <Label text="Destination directory D:\companies" />
</VBox>
